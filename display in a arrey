#include <stdio.h>

int main() {

    int arr2[2][3]; 

    printf(" Array Initialized with User Input (arr2)\n");
    printf("Enter 6 integer elements for a 2x3 matrix:\n");

    
    for (int i = 0; i < 2; i++) { 
        for (int j = 0; j < 3; j++) { 
            printf("Enter element for arr2[%d][%d]: ", i, j);
            scanf("%d", &arr2[i][j]); 
        }
    }

    printf("\nThe entered matrix (arr2):\n");
    
    for (int i = 0; i < 2; i++) { 
        for (int j = 0; j < 3; j++) { 
            printf("%d ", arr2[i][j]);
        }
        printf("\n"); 
    }

    return 0; 
}
